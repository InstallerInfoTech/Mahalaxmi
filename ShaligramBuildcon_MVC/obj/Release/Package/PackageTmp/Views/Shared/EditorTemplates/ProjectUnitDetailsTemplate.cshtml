                        <div class="k-edit-label">
                            @Html.LabelFor(d => d.FloorNumber)
                        </div>
                        <div class="k-edit-field">
                            @(Html.Kendo().NumericTextBoxFor(m => m.FloorNumber)
                                .Spinners(false)
                                .Min(0)
                                .Decimals(4)
                                .HtmlAttributes(new { @tabindex = 1 }))
                            @Html.ValidationMessageFor(m => m.FloorNumber)
                        </div>
                    </td>
                    <td>
                        <div class="k-edit-label">
                            @Html.LabelFor(d => d.FlatNumber)
                        </div>
