                                  .DataTextField("ProjectPlanName")
                                  .DataValueField("ProjectPlanId")
                                  .DataSource(source => source.Read(read => read.Action("GetProjectPlanList", "Common").Data("projectPlanRead")))
                                  .Suggest(true)
                                  .MinLength(0)
                                  .Filter(FilterType.Contains)
                                  .Events(e => e.Change("DisableFields"))
                                  .Placeholder(CommonHelper.PleaseSelect)
